{
  "manifest_version": 2,
  "name": "Lectio - Fraværsoverblik",
  "version": "1.0.0",
  "description": "Udvidelse til Chrome, der gør fraværsoversigter i Lectio mere overskuelige.",
  "author": "Jens Jacob Thomsen",
  "homepage_url": "https://github.com/jensjacobt/lectio-fravaer",
  "content_scripts": [ {
    "matches": [ "https://www.lectio.dk/lectio/*/subnav/fravaergroup.aspx*" ],
    "js": [ "jquery-3.4.1.min.js", "content-script.js" ],
    "run_at": "document_end"
  } ],
  "permissions": [
    "https://www.lectio.dk/lectio/*/subnav/fravaergroup.aspx*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}